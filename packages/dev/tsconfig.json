{
    "compilerOptions": {
        "module": "commonjs",
        "target": "esnext",
        "lib": ["es2018", "dom", "esnext"],
        "moduleResolution": "node",
        "jsx": "react",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitOverride": true,
        "noUncheckedIndexedAccess": true,
        "useUnknownInCatchVariables": false,
        "noEmit": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "declaration": true
    }
}
